{
    AddIdentifierSegment(names, positions) {
        TimeUnit unit = timeUnitCodes.get(names.get(0));
        if (unit != null) {
          return unit;
        }
        throw SqlUtil.newContextException(positions.get(0),
            RESOURCE.invalidDatetimeFormat(SqlIdentifier.getString(names)));
    }
