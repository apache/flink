}

/**
 * Parses a simple identifier as a TimeUnit.
 */
TimeUnit TimeUnitIdentifier() :
{
    final List<String> names = new ArrayList<String>();
    final List<SqlParserPos> positions = new ArrayList<SqlParserPos>();
